{
  "name": "deventy",
  "version": "1.3.0",
  "description": "A minimal 11ty starter with modern tools",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "styles:watch": "postcss src/styles/styles.css -o src/css/style.css && postcss src/styles/styles.css -o src/css/style.css -w",
    "styles:build": "postcss src/styles/styles.css -o src/css/style.css",
    "eleventy:watch": "eleventy --serve --watch",
    "eleventy:build": "eleventy",
    "dev": "npm-run-all -p styles:watch eleventy:watch",
    "surge": "npm run build && surge -d roasted-rings.surge.sh dist",
    "build": "npm-run-all styles:build eleventy:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://ianrose@github.com/ianrose/deventy.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ianrose/deventy/issues"
  },
  "homepage": "https://github.com/ianrose/deventy#readme",
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "light-server": "2.6.4",
    "tailwindcss": "^1.5.2"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "acorn": "^7.3.1",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
